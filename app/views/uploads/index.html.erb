<h1>Listing uploads</h1>

<table>
  <tr>
    <th>User</th>
    <th>Photo file name</th>
    <th>Photo content type</th>
    <th>Photo updated at</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @uploads.each do |upload| %>
  <tr>
    <td><%= upload.user_id %></td>
    <td><%= upload.photo_file_name %></td>
    <td><%= upload.photo_content_type %></td>
    <td><%= upload.photo_updated_at %></td>
    <td><%= link_to 'Show', upload %></td>
    <td><%= link_to 'Edit', edit_upload_path(upload) %></td>
    <td><%= link_to 'Destroy', upload, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Upload', new_upload_path %>
